{
  "name": "openapi2html",
  "version": "1.1.11",
  "description": "Yet another static html generator for Open API 2.0 / Swagger 2.0",
  "main": "src/index.js",
  "author": "Frank Thelen",
  "license": "MIT",
  "homepage": "https://github.com/frankthelen/openapi2html#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frankthelen/openapi2html.git"
  },
  "bugs": {
    "url": "https://github.com/frankthelen/openapi2html/issues"
  },
  "keywords": [
    "swagger",
    "openapi",
    "open api",
    "html",
    "documentation",
    "generator"
  ],
  "scripts": {
    "lint": "eslint src/*.js src/components/*.jsx test/**/*.js --ignore-path ./.eslintignore",
    "test": "NODE_ENV=test mocha --recursive --timeout 3000 test",
    "cover": "NODE_ENV=test nyc --reporter=lcov --reporter=text-summary mocha --exit --recursive --timeout 3000 test",
    "coveralls": "npm run cover && nyc report --reporter=lcovonly && cat ./coverage/lcov.info | coveralls",
    "preversion": "npm run lint && npm test",
    "dev": "npm-run-all --parallel watch:*",
    "watch:server": "nodemon -e js,html --ignore public/ --watch src src/server.js",
    "watch:jsx": "babel --watch -d . ./src/**/*.jsx",
    "postinstall": "babel -d . ./src/**/*.jsx"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "devDependencies": {
    "@hapi/hapi": "^18.3.1",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-should-promised": "^2.0.0",
    "html-entities": "^1.2.1",
    "mocha": "^7.0.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "swagger-parser": "^8.0.0"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "decamelize": "^3.2.0",
    "json-stringify-safe": "^5.0.1",
    "markdown-it": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-render-html": "^0.6.0"
  }
}
